# Colors
local vColorReset="\033[0m"
## Foreground colors for fonts as example
local vColorFgBlack="\033[30m"
local vColorFgRed="\033[31m"
local vColorFgGreen="\033[32m"
local vColorFgBrown="\033[33m"
local vColorFgBlue="\033[34m"
local vColorFgPurple="\033[35m"
local vColorFgCyan="\033[36m"
local vColorFgLightGray="\033[37m"
local vColorFgDarkGray="\033[1;30m"
local vColorFgLightRed="\033[1;31m"
local vColorFgLightGreen="\033[1;32m"
local vColorFgYellow="\033[1;33m"
local vColorFgLightBlue="\033[1;34m"
local vColorFgLightPurple="\033[1;35m"
local vColorFgLightCyan="\033[1;36m"
local vColorFgWhite="\033[1;37m"
## Background colors
local vColorBgBlack="\033[40m"
local vColorBgRed="\033[41m"
local vColorBgGreen="\033[42m"
local vColorBgYellow="\033[43m"
local vColorBgBlue="\033[44m"
local vColorBgMagenta="\033[45m"
local vColorBgCyan="\033[46m"
local vColorBgWhite="\033[47m"

## Custom Paths
local vCustomPath='/Users/user/path/to/tool'

